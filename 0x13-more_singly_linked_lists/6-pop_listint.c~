include "lists.h"
/**
 * pop_listint - delete head node of linked list
 * @head: pointer to first element in  linked list *
 * Return: elementsâ€™ deleted data or 0 if list is empty
 */
int pop_listint(listint_t **head)
{
listint_t *current;
int item;

if (!head || !*head)
return (0);
item = (*head)->n;
current = (*head)->next;
free(*head);
*head = current;
return (item);
}
