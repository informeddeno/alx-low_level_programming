#include "main.h"

/**
 *_strcat- concatenates two strings
 *@dest: input value
 *@src: input value
 *
 *return: void
 */

char *_strcat(char *dest, char *src);
{
int i;int j;

i = 0;
while (i = 0, dest[i] != '\0')
  {
    i++;
  }
j = 0;
while (j = 0, src[i] != '\0)
  {
    dest[i] = src[i];
    i++;
    j++;
  }
 dest[i] != '\0';
 return (dest);
}
